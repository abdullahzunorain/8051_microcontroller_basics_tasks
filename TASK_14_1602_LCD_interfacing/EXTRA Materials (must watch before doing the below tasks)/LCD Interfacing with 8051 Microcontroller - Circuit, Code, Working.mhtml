#define lcd_data P2

sbit rs=3DP0^0;
sbit rw=3DP0^1;
sbit en=3DP0^2;

void lcd_init();
void cmd(unsigned char a);
void dat(unsigned char b);
void show(unsigned char *s);
void lcd_delay();

void lcd_init()
{
    cmd(0x38);
    cmd(0x0e);
    cmd(0x01);
    cmd(0x06);
    cmd(0x0c);
    cmd(0x80);
}

void cmd(unsigned char a)
{
    lcd_data=3Da;
    rs=3D0;
    rw=3D0;
    en=3D1;
    lcd_delay();
    en=3D0;
}

void dat(unsigned char b)
{
    lcd_data=3Db;
    rs=3D1;
    rw=3D0;
    en=3D1;
    lcd_delay();
    en=3D0;
}

void show(unsigned char *s)
{
    while(*s) {
        dat(*s++);
    }
}

void lcd_delay()
{
    unsigned int lcd_delay;
    for(lcd_delay=3D0;lcd_delay&lt;=3D6000;lcd_delay++);
}

int main()
{
    unsigned int j;
    lcd_init();
    while(1) {
        cmd(0x80);
        show("   Welcome To   ");
        cmd(0xc0);
        show(" EMBETRONICX.COM");

        for(j=3D0; j&lt;30000; j++);
        cmd(0x01);
        for(j=3D0; j&lt;30000; j++);
